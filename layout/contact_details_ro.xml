<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="10dp"
    android:layout_marginRight="10dp"
    android:layout_marginTop="10dp"
    android:paddingBottom="5dp">


    <TextView
        android:id="@+id/telephoneRO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/skypeRO"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="1dp"
        android:layout_toEndOf="@+id/teleTitle"
        android:layout_toRightOf="@+id/teleTitle"
        android:background="@color/teal100"
        android:elevation="3dp"
        android:ems="10"
        android:gravity="start"
        android:hint="Telephone Number"
        android:inputType="textPersonName"
        android:paddingBottom="10dp"
        android:paddingLeft="4dp"
        android:paddingTop="10dp"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="144dp"
        tools:layout_editor_absoluteY="68dp" />

    <TextView
        android:id="@+id/teleTitle"
        android:layout_width="20dp"
        android:layout_height="20dp"

        android:background="@drawable/ic_smartphone_call"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="68dp"
        android:layout_alignBaseline="@+id/telephoneRO"
        android:layout_alignBottom="@+id/telephoneRO"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


    <TextView
        android:id="@+id/skypeRO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/telephoneRO"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignStart="@+id/telephoneRO"

        android:layout_below="@+id/facebookRO"
        android:layout_marginTop="1dp"
        android:background="@color/teal100"
        android:elevation="3dp"
        android:ems="10"
        android:gravity="start"
        android:hint="Skype"
        android:inputType="textPersonName"
        android:paddingBottom="10dp"
        android:paddingLeft="4dp"
        android:paddingTop="10dp"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="144dp"
        tools:layout_editor_absoluteY="131dp" />

    <TextView
        android:id="@+id/skypeTitle"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:background="@drawable/ic_skype"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="131dp"
        android:layout_alignBaseline="@+id/skypeRO"
        android:layout_alignBottom="@+id/skypeRO"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


    <TextView
        android:id="@+id/emailTitle"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_alignBaseline="@+id/emailRO"
        android:layout_alignBottom="@+id/emailRO"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/ic_email"
        android:paddingBottom="2dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/facebookRO"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/linkedInRO"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="1dp"
        android:layout_toEndOf="@+id/skypeTitle"
        android:layout_toRightOf="@+id/skypeTitle"
        android:background="@color/teal100"
        android:elevation="3dp"
        android:ems="10"
        android:gravity="start"
        android:hint="Facebook"

        android:inputType="textPersonName"
        android:paddingBottom="10dp"
        android:paddingLeft="6dp"
        android:paddingTop="10dp"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="144dp"
        tools:layout_editor_absoluteY="99dp" />

    <TextView
        android:id="@+id/linkedInRO"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:layout_alignLeft="@+id/facebookRO"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignStart="@+id/facebookRO"
        android:layout_below="@+id/emailRO"
        android:layout_marginTop="1dp"
        android:background="@color/teal100"
        android:elevation="3dp"
        android:ems="10"
        android:gravity="start"
        android:hint="normal"
        android:inputType="textPersonName"
        android:paddingBottom="10dp"
        android:paddingLeft="6dp"
        android:paddingTop="10dp"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="144dp"
        tools:layout_editor_absoluteY="99dp" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_above="@+id/skypeRO"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="8dp"
        app:srcCompat="@drawable/ic_facebook" />


    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_above="@+id/facebookRO"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="8dp"
        app:srcCompat="@drawable/ic_linked_in_square_button" />

    <TextView
        android:id="@+id/emailRO"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:layout_alignLeft="@+id/linkedInRO"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/linkedInRO"
        android:background="@color/teal100"
        android:elevation="3dp"
        android:ems="10"
        android:gravity="start"
        android:hint="email@example.com"
        android:inputType="textPersonName"
        android:paddingBottom="10dp"
        android:paddingLeft="6dp"
        android:paddingTop="10dp"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="144dp"
        tools:layout_editor_absoluteY="99dp" />


</RelativeLayout>